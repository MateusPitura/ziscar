{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nest start --debug --watch",
    "start": "node dist/backend/src/main",
    "build": "nest build",
    "dev:migration-generate": "dotenv -e ../.env -- prisma migrate dev --create-only",
    "dev:migration-run": "dotenv -e ../.env -- prisma migrate deploy && npx prisma generate && npm run generate:enums",
    "prod:db-init": "cross-env NODE_ENV=production ts-node ./prisma/init.ts",
    "dev:db-init": "ts-node ./prisma/init.ts",
    "test:db-init": "cross-env NODE_ENV=test ts-node ./prisma/init.ts",
    "test": "jest",
    "lint": "tsc -b && eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "generate:enums": "cd .. && npm run generate:enums"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.0.16",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/client": "^6.3.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "firebase": "^12.6.0",
    "nestjs-zod": "^4.3.1",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.3",
    "prisma": "^6.3.1",
    "puppeteer": "^24.4.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "globalSetup": "../prisma/connect.ts",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/$1",
      "^@shared/(.*)$": "<rootDir>/../../shared/src/$1"
    },
    "maxWorkers": 1
  }
}
